{
  "name": "logicSolver",
  "version": "1.0.0",
  "description": "Solving Logic Grid Puzzles with Part-of-Speech Tagging and First-Order Logic",
  "license": "MIT",
  "author": {
    "name": "Ross Nordstrom",
    "email": "rnordstr@uccs.edu",
    "url": "http://github.com/ross-nordstrom"
  },
  "main": "cli.js",
  "bin": "cli.js",
  "private": true,
  "scripts": {
    "start": "logicSolver -q",
    "test": "logicSolver -q",
    "testEasy": "logicSolver -q -r -i ../data/puzzlesEasy",
    "testEasy:watch": "nodemon --exec 'npm testEasy'",
    "testModerate": "logicSolver -q -r -i ../data/puzzlesModerate",
    "testModerate:watch": "nodemon --exec 'npm testModerate'",
    "testChallenging": "logicSolver -q -r -i ../data/puzzlesChallenging",
    "testChallenging:watch": "nodemon --exec 'npm testChallenging'"
  },
  "dependencies": {
    "async": "0.9.0",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "diff": "^2.2.1",
    "minimist": "1.1.1",
    "natural": "0.2.1",
    "superagent": "1.2.0",
    "underscore": "1.8.3",
    "underscore.string": "^3.2.2"
  },
  "devDependencies": {
    "expect.js": "0.3.1",
    "istanbul": "0.3.5",
    "jscs": "2.11.0",
    "jsdoc": "3.3.0-beta3",
    "jshint": "2.6.0",
    "mocha": "2.1.0",
    "nodemon": "1.10.2",
    "plato": "1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "github.com/ross-nordstrom/LogicSolver.git"
  },
  "homepage": "https://github.com/ross-nordstrom/LogicSolver",
